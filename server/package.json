{
  "name": "groupinserver",
  "version": "1.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "lint": "eslint . --ext .ts",
    "prepublish": "npm run compile",
    "test": "jest",
    "build": "tsc && npm run copy",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "func start",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "copy": "cp firebase_android_config.json dist && cp src/schema.gql dist/src"
  },
  "nyc": {
    "all": true,
    "include": [
      "server/src/**/*.js",
      "mobile/**/*.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": ".coverage"
  },
  "dependencies": {
    "@types/lodash": "^4.14.165",
    "@types/node": "^16.3.1",
    "apollo-server-azure-functions": "^2.25.2",
    "axios": "^0.19.2",
    "bluebird": "^3.7.1",
    "class-validator": "^0.13.1",
    "firebase-admin": "^7.1.1",
    "graphql": "^15.5.1",
    "lodash": "^4.17.20",
    "md5": "^2.2.1",
    "mobx": "^6.0.4",
    "moment": "^2.22.2",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.15",
    "ts-jest": "^26.4.4",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "dotenv": "^6.2.0",
    "eslint": "^6.8.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "nyc": "^14.1.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=15.0.0",
    "npm": "6.13.1"
  }
}
